@model IEnumerable<QuachHuyDucBTH2.Models.Student>
  <p>
    <a  class="btn btn-success" asp-action="Create">Create a Student</a>
    <a  class="btn btn-success" asp-action="Upload">Upload Students</a>
</p>
<table class="table table-all">
    <tr>
        <th>Student ID</th>
        <th>Student Name</th>
        <th>Student Address</th>
        <th>Student Phone</th>
        <th>Student Email</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.StudentID)</td>
            <td>@Html.DisplayFor(m => item.StudentName)</td>
            <td>@Html.DisplayFor(m => item.StudentAddress)</td>
            <td>@Html.DisplayFor(m => item.StudentPhone)</td>
            <td>@Html.DisplayFor(m => item.StudentEmail)</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StudentID" class="btn btn-success">Sửa</a>
                <a asp-action="Delete" asp-route-id="@item.StudentID" class="btn btn-danger">Xóa</a>
            </td>
        </tr>
    }
</table>